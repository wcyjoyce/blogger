<div class="posts">
  <h1>NEWSFEED</h1>
  <% @user.activity.each do |post| %>
    <div class="post">
      <h3><%= link_to post[:title], post_path(post) %></h3>
      <h6><%= post[:name] %> - <%= post[:created_at] %></h6>
      <p><%= post[:content] %></p>
      <div class="post-tags"><%= raw tag_links(post[:all_tags]) %></div>
    </div>
  <% end %>
</div>


<%= content_for(:title, "Newsfeed") %>
